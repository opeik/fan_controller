{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"args": [ "--release" ],
			"options": {
				"cwd": "${workspaceRoot}/cross"
			},
			"presentation": {
				"clear": true,
			}
		},
	]
}
